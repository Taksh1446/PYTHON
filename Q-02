import csv
student_dict = {}
with open("students.csv", newline='') as file:
    reader = csv.DictReader(file)
    for row in reader:
        total = int(row['Subject1']) + int(row['Subject2']) + int(row['Subject3'])
        student_dict[row['Roll No']] = {
            'Name': row['Name'],
            'Marks': [row['Subject1'], row['Subject2'], row['Subject3']],
            'Total': total
        }
print(student_dict)
