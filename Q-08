class MyString:
    def __init__(self, value):
        self.value = value

    def __iadd__(self, other):
        self.value += other.value
        return self

    def toLower(self):
        return self.value.lower()

    def toUpper(self):
        return self.value.upper()

    def __str__(self):
        return self.value

# Example usage
s1 = MyString("Hello")
s2 = MyString(" World")
s1 += s2
print("Concatenated:", s1)
print("Lowercase:", s1.toLower())
print("Uppercase:", s1.toUpper())
