queue = []
while True:
    print("\nQueue Menu: 1-Enqueue 2-Dequeue 3-Display 4-Exit")
    ch = int(input("Enter choice: "))
    if ch == 1:
        val = input("Enter value to enqueue: ")
        queue.append(val)
    elif ch == 2:
        if queue:
            print("Dequeued:", queue.pop(0))
        else:
            print("Queue is empty")
    elif ch == 3:
        print("Queue:", queue)
    elif ch == 4:
        break
