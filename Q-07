stack = []
while True:
    print("\nStack Menu: 1-Push 2-Pop 3-Display 4-Exit")
    ch = int(input("Enter choice: "))
    if ch == 1:
        val = input("Enter value to push: ")
        stack.append(val)
    elif ch == 2:
        if stack:
            print("Popped:", stack.pop())
        else:
            print("Stack is empty")
    elif ch == 3:
        print("Stack:", stack)
    elif ch == 4:
        break
