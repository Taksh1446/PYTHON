def sin_x(x_deg):
    x = x_deg * 3.14159 / 180
    sin_val = 0
    sign = 1
    for i in range(1, 15, 2):
        num = 1
        den = 1
        for j in range(1, i + 1):
            num *= x
            den *= j
        sin_val += sign * (num / den)
        sign *= -1
    print("sin(x) â‰ˆ", sin_val)
